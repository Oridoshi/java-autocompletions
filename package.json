{
  "name": "java-autocompletions",
  "publisher": "TomDunet",
  "displayName": "Java Autocompl√©tions",
  "description": "Extention java qui contient des snippets Perso",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.83.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Oridoshi/java-autocompletions"
  },
  "categories": [
    "Other",
    "Snippets"
  ],
  "keywords": [
    "java",
    "autocompletion",
    "snippets",
    "java snippets",
    "java autocompletion",
    "java autocomplete",
    "java autocomplete snippets",
    "java autocomplete autocompletion"
  ],
  "activationEvents": [
    "onLanguage:java"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "java",
        "aliases": [
          "JAVA",
          "java"
        ],
        "extensions": [
          ".java"
        ]
      }
    ],
    "snippets": [
      {
        "language": "java",
        "path": "./snippets/java.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.83.0",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "@vscode/test-electron": "^2.3.4",
    "eslint": "^8.47.0",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "ts-loader": "^9.4.4",
    "typescript": "^5.2.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "java-parser": "^2.0.5"
  }
}
